{
    "version": 2,
    "functions": {
        "backend/src/main.ts": {
            "runtime": "@vercel/node",
            "memory": 1024,
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/email/(.*)",
            "dest": "backend/src/main.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}